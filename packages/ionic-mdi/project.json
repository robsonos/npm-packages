{
  "name": "ionic-mdi",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/ionic-mdi/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "ts-node packages/ionic-mdi/src/index.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/packages/ionic-mdi"],
      "options": {
        "jestConfig": "packages/ionic-mdi/jest.config.ts"
      }
    },
    "coverage-badge": {
      "executor": "nx:run-commands",
      "options": {
        "command": "coverage-badges-cli --source {projectRoot}/test-output/jest/coverage/coverage-summary.json --output {projectRoot}/docs/coverage/badges.svg"
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "trackDeps": true,
        "push": true,
        "noVerify": true,
        "skipRootChangelog": true,
        "tagPrefix": "{projectName}@",
        "changelogHeader": ""
      },
      "configurations": {
        "release": {
          "commitMessageFormat": "chore({projectName}): publish release version ${version} [skip ci]",
          "postTargets": [
            "ionic-mdi:build:production",
            "ionic-mdi:publish:release",
            "ionic-mdi:github:release"
          ]
        },
        "prerelease": {
          "releaseAs": "prerelease",
          "commitMessageFormat": "chore({projectName}): publish pre-release version ${version} [skip ci]",
          "postTargets": [
            "ionic-mdi:build:production",
            "ionic-mdi:publish:prerelease",
            "ionic-mdi:github:prerelease"
          ]
        }
      }
    },
    "github": {
      "executor": "@jscutlery/semver:github",
      "options": {
        "tag": "{tag}",
        "notes": "{notes}"
      },
      "configurations": {
        "release": {
          "prerelease": false
        },
        "prerelease": {
          "prerelease": true
        }
      }
    },
    "publish": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "access": "public",
        "distFolderPath": "dist/packages/ionic-mdi",
        "tag": "{preid}",
        "checkExisting": "warning"
      },
      "configurations": {
        "release": {},
        "prerelease": {}
      }
    }
  }
}
